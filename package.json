{
  "name": "react-validatorjs-strategy",
  "version": "0.1.7",
  "description": "Strategy for using validatorjs with react-validation-mixin",
  "main": "./src/strategy.js",
  "browser": "./dist/strategy.js",
  "dependencies": {
    "validatorjs": "2.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.5.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "jasmine-core": "^2.2.0",
    "jasmine-node": "*",
    "uglify-js": "^2.6.1"
  },
  "scripts": {
    "test": "jasmine-node tests",
    "build": "npm run build:cjs && npm run build:umd",
    "build:cjs": "babel ./src/strategy.js --presets es2015 --plugins transform-es2015-modules-commonjs -o ./dist/strategy.js -v",
    "build:umd": "babel ./src/strategy.js --presets es2015 --plugins transform-es2015-modules-umd | uglifyjs -o ./dist/strategy.min.js  --"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheChech/react-validatorjs-strategy.git"
  },
  "keywords": [
    "react",
    "validation",
    "strategy"
  ],
  "author": "Dan Moore",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/TheChech/react-validatorjs-strategy/issues"
  },
  "homepage": "https://github.com/TheChech/react-validatorjs-strategy#readme",
  "engines": {
    "node": ">= 4.0.0"
  }
}
